{
    "name": "spatie/spatie.be",
    "type": "project",
    "description": "Our company website.",
    "keywords": [
        "spatie",
        "company",
        "website",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "abraham/twitteroauth": "^3.3",
        "aws/aws-sdk-php": "^3.240.6",
        "barryvdh/laravel-debugbar": "^3.8",
        "blade-ui-kit/blade-ui-kit": "^0.4",
        "blendbyte/nova-items-field": "^1.0",
        "coroowicaksono/chart-js-integration": "^0.4",
        "doctrine/dbal": "^3.5.1",
        "ebess/advanced-nova-media-library": "dev-master as 4.0.5",
        "geoip2/geoip2": "^2.13",
        "guzzlehttp/guzzle": "^7.5",
        "hammerstone/sidecar": "^0.4",
        "knplabs/github-api": "dev-master#9b01208e1e5500201b45699c911ad53d08f59105",
        "laminas/laminas-feed": "^2.19",
        "laminas/laminas-http": "^2.17",
        "laravel/cashier-paddle": "^1.8",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.12",
        "laravel/nova": "^4.17.1",
        "laravel/sanctum": "^3.3",
        "laravel/slack-notification-channel": "^2.5",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "league/commonmark": "^2.2.1",
        "league/flysystem-aws-s3-v3": "^3.10",
        "livewire/livewire": "^3.0",
        "meilisearch/meilisearch-php": "^1.0",
        "myclabs/php-enum": "^1.8.4",
        "nyholm/psr7": "^1.5.1",
        "ohdearapp/ohdear-php-sdk": "^3.4.4",
        "pda/pheanstalk": "^4.0.4",
        "php-http/guzzle7-adapter": "^1.0",
        "predis/predis": "^1.1.10",
        "psr/http-factory-implementation": "^1.0",
        "spatie/commonmark-shiki-highlighter": "^2.1",
        "spatie/cpu-load-health-check": "^1.0.2",
        "spatie/crypto": "^2.0.1",
        "spatie/eloquent-sortable": "^4.0.1",
        "spatie/fork": "^1.1.2",
        "spatie/holidays": "^1.0",
        "spatie/laravel-artisan-dispatchable": "^1.3",
        "spatie/laravel-backup": "^8.1.5",
        "spatie/laravel-comments": "^1.3",
        "spatie/laravel-comments-livewire": "^2.0",
        "spatie/laravel-enum": "^3.0.1",
        "spatie/laravel-event-sourcing": "^7.3",
        "spatie/laravel-feed": "^4.2",
        "spatie/laravel-flash": "^1.9",
        "spatie/laravel-health": "^1.18",
        "spatie/laravel-help-space": "^1.0",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-markdown": "^2.2.4",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/laravel-menu": "^4.1",
        "spatie/laravel-ray": "^1.32",
        "spatie/laravel-remote": "^1.3.2",
        "spatie/laravel-schedule-monitor": "^3.1",
        "spatie/laravel-site-search": "^2.0",
        "spatie/laravel-slack-alerts": "^1.1.1",
        "spatie/laravel-stubs": "^2.5.2",
        "spatie/laravel-tail": "^4.4",
        "spatie/laravel-validation-rules": "^3.2.1",
        "spatie/packagist-api": "^2.1",
        "spatie/ray": "^1.36",
        "spatie/schema-org": "^3.11",
        "spatie/sheets": "^1.10",
        "spatie/sidecar-shiki": "dev-main",
        "spatie/ssh": "^1.8",
        "spatie/test-time": "^1.3",
        "spatie/valuestore": "^1.3.1",
        "symfony/http-client": "^6.2",
        "symfony/postmark-mailer": "^6.2",
        "symfony/process": "^6.2",
        "torann/geoip": "^3.0.4",
        "wire-elements/spotlight": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.20",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.4",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.2.2",
        "spatie/laravel-blade-comments": "^1.0",
        "spatie/laravel-log-dumper": "^1.4.1",
        "spatie/laravel-visit": "^1.0",
        "spatie/pest-plugin-snapshots": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        },
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "./vendor/bin/pest --parallel",
        "post-install-cmd": [
            "@php composer outdated --direct"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish",
            "@php artisan horizon:publish",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "deploy": "envoy run deploy",
        "deploy-code": "envoy run deploy-code",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "analyse": "vendor/bin/phpstan analyse",
        "baseline": "vendor/bin/phpstan analyse --generate-baseline",
        "ide-helper": [
            "@php artisan ide-helper:eloquent",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "import-writing-readable-php": [
            "php artisan delete-seeded-html-lessons",
            "php ../writing-readable-php-importer/artisan import",
            "php artisan regenerate-html-lessons-html",
            "cp -r ../writing-readable-php-importer/content/images/ public/images/readable-php"
        ]
    }
}
